    <section id="cart_items">
        <div class="container">
            <div class="breadcrumbs">
                <ol class="breadcrumb">
                    <li><a href="/">Home</a></li>
                    <li class="active">Check out</li>
                </ol>
            </div>
            <!--/breadcrums-->
            {{#if this.error}}
            <div class="alert alert-danger" role="alert">
                Invalid Information. <a href="/" class="alert-link">Go back home</a>.
            </div>
            {{else}}
            <div class="review-payment">
                <h2>Apartment Information</h2>
            </div>

            <div class="table-responsive cart_info">
                <table class="table table-condensed">
                    <thead>
                        <tr class="cart_menu">
                            <td class="image">Id</td>
                            <td class="image">Item</td>
                            <td class="description"></td>
                            <td class="price">Price</td>
                            <td class="description">Traders</td>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each this.rooms as |item|}}
                        <tr>
                            <td class="cart_price">
                                <p>{{item.idPhong}}</p>
                            </td>
                            <td class="cart_product">
                                <a href="/areas/{{item.idKhuVuc}}/rooms/{{item.idPhong}}"><img src="{{item.HinhAnh}}"
                                        alt=""></a>
                            </td>
                            <td class="cart_description">
                                <h4><a href="/areas/{{item.idKhuVuc}}/rooms/{{item.idPhong}}">{{item.TenPhong}}</a></h4>
                            </td>
                            <td class="cart_price">
                                <p>{{format item.GiaBan}}</p>
                            </td>
                            <td class="cart_description">
                                <p>{{item.TenNguoiGD}}</p>
                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
            {{/if}}
        </div>
    </section>
