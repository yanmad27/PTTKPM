                <div class="row">
                    <div class="col-sm-9">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse"
                                data-target=".navbar-collapse">
                                <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span
                                    class="icon-bar"></span> <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="mainmenu pull-left">
                            <ul class="nav navbar-nav collapse navbar-collapse">
                                <li><a href="/"><i class="fa fa-home"></i>Home</a></li>
                                {{#if isAuthenticated}}
                                <li class="dropdown"><a href="#">Hi, <b>{{authUser.TenDangNhap}}</b><i
                                            class="fa fa-angle-down"></i></a>
                                    <ul role="menu" class="sub-menu">
                                        <li><a href="/account/profile/{{authUser.idKHACHHANG}}">
                                                <i class="fa fa-user"></i>
                                                Profile
                                            </a></li>
                                        <li><a href="/account/history/{{authUser.idKHACHHANG}}">
                                                <i class="fa fa-shopping-cart"></i>
                                                History
                                            </a></li>
                                        {{#ifEq authUser.Quyen 1}}
                                        <li><a href="/admin"><i class="fa fa-lock"></i>
                                                Admin</a></li>
                                        {{/ifEq}}
                                        <li><a href="/account/changepass">
                                                <i class="fa fa-key"></i>
                                                Change Password
                                            </a></li>
                                        <li><a href="javascript: $('#frmSignout').submit();">
                                                <i class="fa fa-sign-out"></i>
                                                Sign Out
                                            </a></li>
                                    </ul>
                                </li>
                                {{else}}
                                <li><a href="/account/register"><i class="fa fa-user-plus"></i>
                                        Register</a></li>
                                <li><a href="/account/login"><i class="fa fa-sign-in"></i>
                                        Sign In</a></li>
                                {{/if}}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
